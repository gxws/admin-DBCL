  body.no-skin
    ///#section:basics/navbar.layout
    include _nav
    div#main-container.main-container
      //<script type="text/javascript">try{ace.settings.check('main-container' , 'fixed')}catch(e){}</script>
      include _sidebar
      div.main-content
        //#section:basics/content.breadcrumbs
        div#breadcrumbs.breadcrumbs
          //<script type="text/javascript">try{ace.settings.check('breadcrumbs' , 'fixed')}catch(e){}</script>
          ul.breadcrumb
            li
              i.ace-icon.fa.fa-home.home-icon
              a(href="#") 首页
            li
              a(href="#") 系统
            li
              a(href="#") 分类
            li.active
              a(href="#") 当前页
        ///section:basics/content.breadcrumbs
        div.page-content
          div#ace-settings-container.ace-settings-container
            div#ace-settings-btn.btn.btn-app.btn-xs.btn-warning.ace-settings-btn
              i.ace-icon.fa.fa-cog.bigger-150
            div#ace-settings-box.ace-settings-box.clearfix
              div.pull-left.width-50
                div.ace-settings-item
                  input#ace-settings-navbar.ace.ace-checkbox-2(type="checkbox")
                  label.lbl(for="ace-settings-navbar") 冻结顶栏
                div.ace-settings-item
                  input#ace-settings-sidebar.ace.ace-checkbox-2(type="checkbox")
                  label.lbl(for="ace-settings-sidebar") 冻结边栏
                div.ace-settings-item
                  input#ace-settings-breadcrumbs.ace.ace-checkbox-2(type="checkbox")
                  label.lbl(for="ace-settings-breadcrumbs") 冻结面包屑
                div.ace-settings-item
                  input#ace-settings-container.ace.ace-checkbox-2(type="checkbox")
                  label.lbl(for="ace-settings-container") 页面居中对齐
              div.pull-left.width-50
                div.ace-settings-item
                  input#ace-settings-hover.ace.ace-checkbox-2(type="checkbox")
                  label.lbl(for="ace-settings-hover") 鼠标经过时弹出菜单
                div.ace-settings-item
                  input#ace-settings-compact.ace.ace-checkbox-2(type="checkbox")
                  label.lbl(for="ace-settings-compact") 窄边栏
                div.ace-settings-item
                  input#ace-settings-highlight.ace.ace-checkbox-2(type="checkbox")
                  label.lbl(for="ace-settings-highlight") 边栏菜单突出
          div.page-content-area
            div.row
              include _service_fulfillment_record
      a#btn-scroll-up.btn-scroll-up.btn.btn-sm.btn-inverse(href="#")
        i.ace-icon.fa.fa-angle-double-up.icon-only.bigger-110
