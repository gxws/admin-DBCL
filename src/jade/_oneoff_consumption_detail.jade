div.col-sm-12
  //PAGE CONTENT BEGINS
  div(class="title-line clearfix")
    span.h1 Title
  - var datas = [{title:'订单信息',names:['支付单号','订单编号','订单名称','订单状态','付款金额','开通时间','创建时间','失败原因']},{title:'产品信息',names:['服务商编号','客户编号','产品ID','产品名称']},{title:'用户信息',names:['机顶盒号','用户编号','用户区域码','机顶盒类型']}]
  - for data in datas
    table(class="table table-bordered mt20")
      thead
        tr.active
          th(colspan="2")= data.title
      tbody
        - for name in data.names
          tr
            td(class="text-right info width-15")= name
            td= '{' + name + '}'
  a(href="javascript:;" class="btn btn-sm mr20") 返回